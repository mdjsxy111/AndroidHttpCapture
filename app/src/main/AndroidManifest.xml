<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.accmobile.httpcapturedemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.accmobile.httpcapturedemo.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DebuggingDemo"
        tools:replace="android:theme">
        <activity android:name="com.accmobile.httpcapturedemo.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.accmobile.httpcapturedemo.activity.PostVideoListActivity" />
        <activity android:name="com.accmobile.httpcapturedemo.activity.GetSingleActivity" />
        <activity android:name="com.accmobile.httpcapturedemo.activity.WebViewActivity" />
    </application>

</manifest>